
bool boolTest() {
  bool ok;
  ok = true;
  ok = ok && (true && true);
  ok = ok && ((true && false) == false);
  ok = ok && ((false && true) == false);
  ok = ok && ((false && false) == false);
  ok = ok && (true || true);
  ok = ok && (true || false);
  ok = ok && (false || true);
  ok = ok && ((false || false) == false);
  return ok;
}
